[32m+[m[32m            logger.websocketMessage('RECEIVE', message.type, message);[m
[32m+[m
[32m+[m[32m            switch (message.type) {[m
[32m+[m[32m              case 'queue_update':[m
[32m+[m[32m                if (message.queue) {[m
[32m+[m[32m                  setWaitingQueue(message.queue);[m
[32m+[m[32m                }[m
[32m+[m[32m                break;[m
[32m+[m
[32m+[m[32m              case 'chat_message':[m
[32m+[m[32m                if (message.message) {[m
[32m+[m[32m                  setChatMessages(prev => [...prev, {[m
[32m+[m[32m                    id: Date.now().toString(),[m
[32m+[m[32m                    userId: message.userId || '',[m
[32m+[m[32m                    username: message.userName || 'AnÃ´nimo',[m
[32m+[m[32m                    message: message.message,[m
[32m+[m[32m                    timestamp: new Date(),[m
[32m+[m[32m                    type: 'user'[m
[32m+[m[32m                  }]);[m
[32m+[m[32m                }[m
[32m+[m[32m                break;[m
[32m+[m
[32m+[m[32m              case 'match_found':[m
[32m+[m[32m                logger.info('LOBBY', 'Match found, navigating to game', { gameId: message.game_id });[m
[32m+[m[32m                if (message.game_id) {[m
